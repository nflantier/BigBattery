{
    "multipart": [
        {   "when": {"type" : "basic"},
            "apply": { "model": "bigbattery:blockbasiccasing"}
        },
        
        {   "when": {"OR" :[{ "down": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-d"}
        },
        {   "when": { "down": "true","isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-dt"}
        },
        {   "when": {"OR" :[{ "up": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-u", "x": 180  }
        },
        {   "when": { "up": "true" ,"isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-ut", "x": 180  }
        },
        
        {   "when": {"OR" :[{ "north": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-n"}
        },
        {   "when": { "north": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-nt"}
        },
        {   "when": {"OR" :[{ "south": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-s", "y": 180}
        },
        {   "when": { "south": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-st", "y": 180}
        },
        
        {   "when": {"OR" :[{ "west": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-w"}
        },
        {   "when": { "west": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-wt"}
        },
        {   "when": {"OR" :[{ "east": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-e", "y": 180}
        },
        {   "when": { "east": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-et", "y": 180}
        }
    ]
}