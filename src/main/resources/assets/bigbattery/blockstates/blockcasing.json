{
    "multipart": [
        {   "when": {"type" : "glass"},
            "apply": { "model": "bigbattery:blockbasiccasing"}
        },
        {   "when": {"type" : "iron"},
            "apply": { "model": "bigbattery:blockadvancedcasing"}
        },
        
        {   "when": {"OR" :[{ "down": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-d"}
        },
        {   "when": { "type" : "glass", "down": "true","isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-dt"}
        },
        {   "when": {"OR" :[{ "up": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-u", "x": 180  }
        },
        {   "when": { "type" : "glass", "up": "true" ,"isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-ut", "x": 180  }
        },
        
        {   "when": {"OR" :[{ "north": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-n"}
        },
        {   "when": { "type" : "glass", "north": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-nt"}
        },
        {   "when": {"OR" :[{ "south": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-s", "y": 180}
        },
        {   "when": { "type" : "glass", "south": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-st", "y": 180}
        },
        
        {   "when": {"OR" :[{ "west": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-w"}
        },
        {   "when": { "type" : "glass", "west": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-wt"}
        },
        {   "when": {"OR" :[{ "east": "false" }, {"isstruct" : "false"}]},
            "apply": { "model": "bigbattery:blockbasiccasing-e", "y": 180}
        },
        {   "when": { "type" : "glass", "east": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockbasiccasing-et", "y": 180}
        },
        
        
        {   "when": { "type" : "iron", "down": "true","isstruct" : "true"},
            "apply": { "model": "bigbattery:blockadvancedcasing-dt"}
        },
        {   "when": { "type" : "iron", "up": "true" ,"isstruct" : "true"},
            "apply": { "model": "bigbattery:blockadvancedcasing-ut", "x": 180  }
        },
        
        {   "when": { "type" : "iron", "north": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockadvancedcasing-nt"}
        },
        {   "when": { "type" : "iron", "south": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockadvancedcasing-st", "y": 180}
        },
        
        {   "when": { "type" : "iron", "west": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockadvancedcasing-wt"}
        },
        {   "when": { "type" : "iron", "east": "true" , "isstruct" : "true"},
            "apply": { "model": "bigbattery:blockadvancedcasing-et", "y": 180}
        }
    ]
}